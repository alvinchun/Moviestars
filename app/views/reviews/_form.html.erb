<%= simple_form_for([@play, @play.reviews.build]) do |f| %>
  <div id="rating-form">
    <label> Rating </label>
  </div>
  <%= f.input :comment %>
  <%= f.button :submit, :class => "btn-custom3" %>
<% end %> 

<script>
 $('#rating-form').raty({
   path: '/image/ratyrate',
   scoreName: 'review[rating]'
 });
</script>


